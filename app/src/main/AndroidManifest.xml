        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".starting_ACT">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Admin_ACT"/>
        <activity android:name=".Computing"/>
        <activity android:name=".Login_ACT"/>
        <activity android:name=".MainActivity"/>
        <activity android:name=".Subject2_show"/>
        <activity android:name=".View_Computing"/>
        <activity android:name=".View_Prog"/>
        <activity android:name=".Updation_ACT"/>
        <activity android:name=".Updata_LV"/>
        <activity android:name=".subject1_ShowACT"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

    </application>

</manifest>